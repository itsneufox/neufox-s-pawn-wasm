{
  "name": "@pawn-fiddle/pawn-wasm",
  "version": "1.0.0",
  "description": "WebAssembly build of the Pawn Community Compiler for use in browsers",
  "type": "module",
  "main": "dist/pawn-compiler.js",
  "types": "dist/pawn-compiler.d.ts",
  "files": [
    "dist",
    "src",
    "build.sh",
    "CMakeLists.txt",
    "tsconfig.json"
  ],
  "scripts": {
    "build:wasm": "bash build.sh",
    "build:js": "tsc",
    "build": "npm run build:wasm && npm run build:js",
    "clean": "rm -rf dist build/output",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/pawn-wasm.git"
  },
  "keywords": [
    "pawn",
    "compiler",
    "webassembly",
    "wasm",
    "sa-mp",
    "open.mp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/pawn-wasm/issues"
  },
  "homepage": "https://github.com/yourusername/pawn-wasm#readme",
  "devDependencies": {
    "@types/emscripten": "^1.39.13",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
